pipeline {
	agent any
		stages {
    	    stage("Stage 1") {   
        	    steps {               	 
            	        echo 'Hello from Pipeline'	 
           	   	}    
    	    }
    	    stage('Stage 2') {
        	    steps {
        	        echo 'Hello from Stage 2 in the pipeline'  	 
        	    }
    	    }
    	
        }

}
